#include <iostream>

using namespace std;

//Compiler version g++ 6.3.0

int main()

{

  int num;

  int div=2;

  

    cout <<"Enter the number to check prime or Note : "<<endl;

    cin>>num;

    

    while(div<num)

    {

      if (num% div==0) 

      {

        cout<<"the number is not prime";

        return 0;

        

      }

     div++;

      

   

    }

    cout<<"the number is prime";

    

}
